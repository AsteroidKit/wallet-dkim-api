{
  "compilerOptions": {
    "target": "es2016",
    "types": ["node"],
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "noImplicitAny": false,
    "resolveJsonModule": true,
    "lib": ["es6"],
    "allowJs": true,
    "outDir": "build"
  }
}
